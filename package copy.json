    "@nestjs/testing": "^11.0.5",
    "dotenv": "^16.5.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "ts-node-dev": "^2.0.0",
